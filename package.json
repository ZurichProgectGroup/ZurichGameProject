{
  "name": "zurichgameproject",
  "version": "1.0.0",
  "description": "Yandex Praktikum Team game project",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start:webpack": "webpack --mode=development --watch",
    "start:server": "nodemon index.js --watch dist/server.js",
    "start": "NODE_ENV=development npm-run-all --print-label --parallel start:*",
    "build": "NODE_ENV=production npm run clean && webpack --mode production",
    "lint-js": "eslint . --ext .js,.jsx,.ts,.tsx ",
    "lint-styles": "stylelint \"**/*.css\"",
    "lint:fix": "npm run lint-js -- --fix && npm run lint-styles -- --fix",
    "buildback": "npx tsc --p ./backend/",
    "initDB": "node ./backend/dist/initPostgresDB.js && npx sequelize-cli db:migrate",
    "serveback": "nodemon ./backend/dist/app.js",
    "startback": "npm run buildback  && npm run serveback",
    "test": "jest"
  },
  "scriptsComments": {
    "start": "add port 5000 for yandex oauth authorisation, becase it always redirects to http://localhost:5000/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZurichProgectGroup/ZurichGameProject.git"
